  "keywords": [
    "afterpay",
    "payments",
    "shopping"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Jared Fraser",
      "email": "j.fraser@culturekings.com.au"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:culturekings/Symfony2-coding-standard.git"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=8.1",
    "guzzlehttp/guzzle": "^7.0",
    "jms/serializer": "*",
